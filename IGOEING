local TweenService = game:GetService("TweenService")

ScrollingFrame.CanvasPosition = Vector2.new(0, 0)

Frame2.Visible = false

--dragging
local UserInputService = game:GetService("UserInputService")
local VirtualInputManager=game:GetService("VirtualInputManager")
local TeleportService = game:GetService("TeleportService")
local Mouse = game.Players.LocalPlayer:GetMouse()

local gui = Frame2

local dragging
local dragInput
local dragStart
local startPos

local folder = Instance.new("Folder",workspace)
folder.Name = "IslandLocations"

local function update(input)
	local delta = input.Position - dragStart
	gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

gui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

gui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)
--dragging

local function billboard(text,parent,color)
	if parent:FindFirstChild("BillboardGui") then return end
	local BillboardGui = Instance.new("BillboardGui")
	local TextLabel = Instance.new("TextLabel")

	--Properties:

	BillboardGui.Parent = parent
	BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	BillboardGui.Active = true
	BillboardGui.AlwaysOnTop = true
	BillboardGui.LightInfluence = 1.000
	BillboardGui.Size = UDim2.new(0, 200, 0, 50)

	TextLabel.Parent = BillboardGui
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.Size = UDim2.new(0, 200, 0, 50)
	TextLabel.Font = Enum.Font.SourceSansBold
	TextLabel.TextColor3 = color
	TextLabel.TextSize = 20.000
	TextLabel.Text = text
	TextLabel.TextStrokeTransparency = 0
end

local function teleportfunction(name)
	local tweenInfo = TweenInfo.new(
		4, -- Time
		Enum.EasingStyle.Linear, -- EasingStyle
		Enum.EasingDirection.Out, -- EasingDirection
		0, -- RepeatCount (when less than zero the tween will loop indefinitely)
		true, -- Reverses (tween will reverse once reaching it's goal)
		0 -- DelayTime
	)

	local tween = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, tweenInfo, {CFrame = workspace:FindFirstChild(name).Handle.CFrame})
	tween:Play()
end

--Animate
Frame:TweenSizeAndPosition(UDim2.new(0.216, 0,0.134, 0), UDim2.new(0.391, 0,0.412, 0))
TextLabel.Transparency = 1
TextLabel2.Transparency = 1
TextLabel.Visible = true
TextLabel2.Visible = true
wait(1)
local tween = TweenService:Create(TextLabel, TweenInfo.new(.5), {Transparency = 0})
local tween2 = TweenService:Create(TextLabel2, TweenInfo.new(.5), {Transparency = 0})

tween:Play()
tween2:Play()
wait(2)
Frame:TweenSizeAndPosition(UDim2.new(0, 0,0, 0), UDim2.new(0.500, 0,0.500, 0))
local tween3 = TweenService:Create(TextLabel, TweenInfo.new(.5), {Transparency = 1})
local tween4 = TweenService:Create(TextLabel2, TweenInfo.new(.5), {Transparency = 1})

tween3:Play()
tween4:Play()
wait(1)
Frame.Visible = false
Frame2.Visible = true
ImageButton.Visible = true

ImageButton.MouseButton1Click:Connect(function()
	Frame2.Visible = not Frame2.Visible
	if ImageButton.Image == "rbxassetid://7027303483" then
		ImageButton.Image = "rbxassetid://7027534015"
		local audio = Instance.new("Sound",workspace)
		audio.SoundId = "rbxassetid://6548799955"
		audio:Play()
		ImageButton.Size = UDim2.new(0.025, 0,0.04, 0)
		ImageButton.Position = UDim2.new(0.063, 0,0.560, 0)
		ImageButton:TweenSizeAndPosition(UDim2.new(0.0203230847, 0, 0.0361111127, 0), UDim2.new(0.065, 0,0.562, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.3)
		repeat wait() until audio.Playing == false
		audio:Destroy()
	else
		ImageButton.Image = "rbxassetid://7027303483"
	end
end)

settingsopen = false
settings.MouseButton1Click:Connect(function()
	if settingsopen == false then
		settingsopen = true
		selected:TweenSizeAndPosition(UDim2.new(1.001, 0,0.23, 0), UDim2.new(-0.001, 0,0.218, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
	end
end)

fruitbutton.MouseButton1Click:Connect(function()
	game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Cousin","Buy")
end)

shopbutton.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.PlayerGui.Main.FruitShop.Visible = true
end)

rejoinbutton.MouseButton1Click:Connect(function()
	TeleportService:Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
end)

section1true = true
section2true = false
section3true = false
waiting = false

local canvaspos = TweenInfo.new(
	.35, -- Time
	Enum.EasingStyle.Linear, -- EasingStyle
	Enum.EasingDirection.InOut, -- EasingDirection
	0, -- RepeatCount (when less than zero the tween will loop indefinitely)
	false, -- Reverses (tween will reverse once reaching it's goal)
	0 -- DelayTime
)

togglesbutton.MouseButton1Down:Connect(function()
	if section2true == false then
		if waiting == true then return end
		waiting = true
		section1true = false
		section3true = false
		section2true = true
		local tween = TweenService:Create(ScrollingFrame, canvaspos, {CanvasPosition = Vector2.new(0, 410)})
		tween:Play()
		section1.Visible = false
		section2.Visible = true
		section3.Visible = false
		line1:TweenSizeAndPosition(UDim2.new(0, 0,0.114, 0), UDim2.new(0.481, 0,0.78, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
		line2:TweenSizeAndPosition(UDim2.new(0.808, 0,0.114, 0), UDim2.new(0.117, 0,0.78, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
		line3:TweenSizeAndPosition(UDim2.new(0, 0,0.114, 0), UDim2.new(0.481, 0,0.78, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
		wait(.5)
		waiting = false
	end
end)



mainbutton.MouseButton1Down:Connect(function()
	if section1true == false then
		if waiting == true then return end
		waiting = true
		section2true = false
		section3true = false
		section1true = true
		local tween = TweenService:Create(ScrollingFrame, canvaspos, {CanvasPosition = Vector2.new(0, 0)})
		tween:Play()
		section1.Visible = true
		section2.Visible = false
		section3.Visible = false
		line1:TweenSizeAndPosition(UDim2.new(0.808, 0,0.114, 0), UDim2.new(0.117, 0,0.78, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
		line2:TweenSizeAndPosition(UDim2.new(0, 0,0.114, 0), UDim2.new(0.481, 0,0.78, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
		line3:TweenSizeAndPosition(UDim2.new(0, 0,0.114, 0), UDim2.new(0.481, 0,0.78, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
		wait(.5)
		waiting = false
	end
end)

devilfruitbutton.MouseButton1Down:Connect(function()
	if section3true == false then
		if waiting == true then return end
		waiting = true
		section2true = false
		section3true = true
		section1true = false
		local tween = TweenService:Create(ScrollingFrame, canvaspos, {CanvasPosition = Vector2.new(0, 820)})
		tween:Play()
		section1.Visible = false
		section2.Visible = false
		section3.Visible = true
		line1:TweenSizeAndPosition(UDim2.new(0, 0,0.114, 0), UDim2.new(0.481, 0,0.78, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
		line2:TweenSizeAndPosition(UDim2.new(0, 0,0.114, 0), UDim2.new(0.481, 0,0.78, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
		line3:TweenSizeAndPosition(UDim2.new(1, 0,0.114, 0), UDim2.new(0, 0,0.78, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.5)
		wait(.5)
		waiting = false
	end
end)

local function enable(button)
	button:FindFirstChildOfClass("ImageLabel").X.TextColor3 = Color3.fromRGB(0, 255, 0)
	button:FindFirstChildOfClass("ImageLabel").X.Text = "✓"
	button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
	button:FindFirstChildOfClass("ImageLabel"):TweenSizeAndPosition(UDim2.new(0.373, 0,0.819, 0), button:FindFirstChildOfClass("ImageLabel").Position + UDim2.new(.55, 0, 0, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.6)
end

local function disable(button)
	button:FindFirstChildOfClass("ImageLabel").X.TextColor3 = Color3.fromRGB(255, 0, 0)
	button:FindFirstChildOfClass("ImageLabel").X.Text = "x"
	button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	button:FindFirstChildOfClass("ImageLabel"):TweenSizeAndPosition(UDim2.new(0.373, 0,0.819, 0), button:FindFirstChildOfClass("ImageLabel").Position - UDim2.new(.55, 0, 0, 0),Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.6)
end

local locating = false
local locatecd = false
locateon.MouseButton1Click:Connect(function()
	if locatecd == true then return end
	locatecd = true
	if locating == false then
		locating = true
		enable(locateon)
		local findplayer = workspace.Characters:FindFirstChild(locatebutton.Text)
		if findplayer then
			billboard(findplayer.Name,findplayer.Head,Color3.fromRGB(74,86,255))
		end
	elseif locating == true then
		locating = false
		disable(locateon)
		local findplayer = workspace.Characters:FindFirstChild(locatebutton.Text)
		if findplayer then
			if findplayer.Head:FindFirstChild("BillboardGui") then
				findplayer.Head.BillboardGui:Destroy()
			end
		end
	end
	wait(.6)
	locatecd = false
end)

