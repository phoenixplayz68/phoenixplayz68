local espfruit = false
local fruitcd = false
fruitespbutton.MouseButton1Click:Connect(function()
	if fruitcd == true then return end
	fruitcd = true
	if espfruit == false then
		enable(fruitespbutton)
	else
		disable(fruitespbutton)
	end
	if espfruit == false then
		espfruit = true
		for i,v in pairs(workspace:GetChildren()) do
			if v.ClassName == "Tool" then
				billboard(v.Name,v.Handle,Color3.fromRGB(255,0,0))
			end
			if v.Name == "Fruit " then
				if v:FindFirstChild("Spike") then
					billboard("Spike Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v:FindFirstChild("Coil") then
					billboard("Spring Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v:FindFirstChild("Leaves") then
					billboard("Bomb Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Lavender") then
					billboard("Chop Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Medium stone grey") then
					billboard("Smoke Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Bright orange") and v.Stem.BrickColor == BrickColor.new("Bright bluish green") then
					billboard("Flame Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Teal") and v.Stem.BrickColor == BrickColor.new("Pastel blue-green") then
					billboard("Ice Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Royal purple") and v.Stem.BrickColor == BrickColor.new("Earth green") then
					billboard("Dark Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("New Yeller") then
					billboard("Light Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Burlap") and v.Stem.BrickColor == BrickColor.new("Bright bluish green") then
					billboard("Sand Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Shamrock") then
					billboard("Barrier Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Cocoa") and v.Stem.BrickColor == BrickColor.new("Earth green") then
					billboard("Magma Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Institutional white") and v.Stem.BrickColor == BrickColor.new("Medium stone grey") then
					billboard("Quake Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Institutional white") and v.Stem.BrickColor == BrickColor.new("Hot pink") then
					billboard("String Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Buttermilk") and v.Stem.BrickColor == BrickColor.new("Brown") then
					billboard("Dough Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Pink") and v.Stem.BrickColor == BrickColor.new("Brown") then
					billboard("Rubber Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Toothpaste") and v.Stem.BrickColor == BrickColor.new("Brown") then
					billboard("Rumble Fruit",v.Handle,Color3.fromRGB(255,0,0))
				elseif v.Fruit.BrickColor == BrickColor.new("Cork") and v.Stem.BrickColor == BrickColor.new("Reddish brown") then
					billboard("Buddha Fruit",v.Handle,Color3.fromRGB(255,0,0))

				else
					billboard(v.Name,v.Handle,Color3.fromRGB(255,0,0))
				end
			end
		end
	elseif espfruit == true then
		espfruit = false
		for i,v in pairs(workspace:GetChildren()) do
			if v.Name == "Fruit " then
				v.Handle.BillboardGui:Destroy() 
			end
			if v.ClassName == "Tool" then
				v.Handle.BillboardGui:Destroy() 
			end
		end
	end
	wait(.6)
	fruitcd = false
end)

local playeresp = false
local playercd = false
personespbutton.MouseButton1Click:Connect(function()
	if playercd == true then return end
	playercd = true
	if playeresp == false then
		playeresp = true
		enable(personespbutton)
		for i,v in pairs(workspace.Characters:GetChildren()) do
			if v.Name ~= game.Players.LocalPlayer.Name then
				billboard(v.Name,v.Head,Color3.fromRGB(74,86,255))
			end
		end
	elseif playeresp == true then
		playeresp = false
		disable(personespbutton)
		for i,v in pairs(workspace.Characters:GetChildren()) do
			if v.Head:FindFirstChild("BillboardGui") then
				v.Head.BillboardGui:Destroy()
			end
		end
	end
	wait(.6)
	playercd = false
end)

local autodashing = false
local pressingq = false
local closed = false

autodashbutton.MouseButton1Click:Connect(function()
	if playercd == true then return end
	playercd = true
	if autodashing == false then
		autodashing = true
		enable(autodashbutton)
		coroutine.resume(coroutine.create(function()
			repeat wait(.05)
				if pressingq == true and closed == false then
					VirtualInputManager:SendKeyEvent(true,"Q",false,game)
				end
			until autodashing == false
		end))
	elseif autodashing == true then
		autodashing = false
		disable(autodashbutton)
	end
	wait(.6)
	playercd = false
end)

game:GetService("UserInputService").InputBegan:connect(function(input)
	if input.KeyCode == Enum.KeyCode.Q then
		pressingq = true
	end
end)

game:GetService("UserInputService").InputEnded:connect(function(input)
	if input.KeyCode == Enum.KeyCode.Q then
		pressingq = false
	end
end)

close.MouseButton1Click:Connect(function()
	if playeresp == true then
		closed = true
		playeresp = false
		locating = false
		autodashing = false
		pressingq = false
		for i,v in pairs(workspace.Characters:GetChildren()) do
			if v.Head:FindFirstChild("BillboardGui") then
				v.Head.BillboardGui:Destroy()
			end
		end
	end
	if espfruit == true then
		espfruit = false
		for i,v in pairs(workspace:GetChildren()) do
			if v.Name == "Fruit " then
				v.Handle.BillboardGui:Destroy() 
			end
			if v.ClassName == "Tool" then
				v.Handle.BillboardGui:Destroy() 
			end
		end
	end
	if islandesp == true then
		islandesp = false
	end
	ScreenGui:Destroy()
	folder:Destroy()
end)

workspace.ChildAdded:Connect(function(child)
	if espfruit == true then
		if child.ClassName == "Tool" then
			billboard(child.Name,child.Handle,Color3.fromRGB(255,0,0))
		end
		if child.Name == "Fruit " then
			if child:FindFirstChild("Spike") then
				billboard("Spike Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child:FindFirstChild("Coil") then
				billboard("Spring Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child:FindFirstChild("Leaves") then
				billboard("Bomb Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Lavender") then
				billboard("Chop Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Medium stone grey") then
				billboard("Smoke Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Bright orange") and child.Stem.BrickColor == BrickColor.new("Bright bluish green") then
				billboard("Flame Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Teal") and child.Stem.BrickColor == BrickColor.new("Pastel blue-green") then
				billboard("Ice Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Royal purple") and child.Stem.BrickColor == BrickColor.new("Earth green") then
				billboard("Dark Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("New Yeller") then
				billboard("Light Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Burlap") and child.Stem.BrickColor == BrickColor.new("Bright bluish green") then
				billboard("Sand Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Shamrock") then
				billboard("Barrier Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Cocoa") and child.Stem.BrickColor == BrickColor.new("Earth green") then
				billboard("Magma Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Institutional white") and child.Stem.BrickColor == BrickColor.new("Medium stone grey") then
				billboard("Quake Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Institutional white") and child.Stem.BrickColor == BrickColor.new("Hot pink") then
				billboard("String Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Buttermilk") and child.Stem.BrickColor == BrickColor.new("Brown") then
				billboard("Dough Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Pink") and child.Stem.BrickColor == BrickColor.new("Brown") then
				billboard("Rubber Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Toothpaste") and child.Stem.BrickColor == BrickColor.new("Brown") then
				billboard("Rumble Fruit",child.Handle,Color3.fromRGB(255,0,0))
			elseif child.Fruit.BrickColor == BrickColor.new("Cork") and child.Stem.BrickColor == BrickColor.new("Reddish brown") then
				billboard("Buddha Fruit",child.Handle,Color3.fromRGB(255,0,0))
			else
				billboard(child.Name,child.Handle,Color3.fromRGB(255,0,0))
			end
		end
	end
end)

repeat wait() until workspace:FindFirstChild("Characters")
workspace.Characters.ChildAdded:Connect(function(child)
	if playeresp == true then
		if child.Name ~= game.Players.LocalPlayer.Name then
			billboard(child.Name,child.Head,Color3.fromRGB(74,86,255))
		end
	end
	local findplayer = workspace.Characters:FindFirstChild(locatebutton.Text)
	if findplayer and locating == true then
		billboard(locatebutton.Text,findplayer.Head,Color3.fromRGB(74,86,255))
	end
end)
