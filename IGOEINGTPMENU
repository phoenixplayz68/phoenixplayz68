local bring = false

local updateBring = function()
	UIStroke.Color=bring and Color3.new(0,255,0) or Color3.fromRGB(255,0,0)
	UIStroke2.Color=bring and Color3.new(0,255,0) or Color3.fromRGB(255,0,0)
	TextButton.Text=bring and "✓" or "X"
	TextButton.TextColor3=bring and Color3.fromRGB(0,255,0) or Color3.fromRGB(255,0,0)
end
updateBring()

local folder = nil
local npcFolder = (game.GameId==6099256669 or game.GameId==2217006564) and workspace.Other or nil
local addCon = nil
local rmCon = nil

for i,v in pairs(workspace:GetChildren()) do
	local name=string.lower(v.Name)
	if (name=="npcs" or name=="npc" or name=="npc's") and v:IsA("Folder") then
		npcFolder=v break
	end
end

local val=0

local getNum=function(frame,parent)
	for i,v in pairs(frame:GetChildren()) do
		if v:IsA("TextButton") then
			local num = 0
			local num2 = 0
			local Return = false
			for e,g in pairs(parent:GetChildren()) do
				for f,j in pairs(v:GetChildren()) do
					if j.ClassName=="Script" and j.Name=="Properties" then
						Return = false break
					elseif j.ClassName=="LocalScript" and j.Name=="Properties" then
						num2+=1
					end
				end
				if num2>=2 or Return then continue end
				if g.Name==v.Name then num+=1 end
			end
			v.amt.Text = "AMT: "..num
		end
	end
end

local createItem = function(obj,parent,noDupes)
	if noDupes and parent:FindFirstChild(obj.Name) then return end
	local clone=Template:Clone()
	clone.Name=obj.Name
	clone.Parent=parent
	clone.TextLabel.Text=obj.Name
	clone.Visible=true
	parent.CanvasSize=UDim2.new(0,0,0,parent.UIListLayout.AbsoluteContentSize.Y)
	return clone
end

local buttonCons = {}

local buttonFunc = function(button,obj,skip)
	task.spawn(function()
		if button==nil then return end
		local con
		local con2
		local par = obj.Parent
		local name = obj.Name
		con = button.MouseButton1Click:Connect(function()
			local child=par:FindFirstChild(name)
			if child==nil then return end
			if child:FindFirstChild("HumanoidRootPart") then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=child.HumanoidRootPart.CFrame
				return
			end
			if bring then
				if child.PrimaryPart~=nil then
					child:SetPrimaryPartCFrame(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
				elseif child:FindFirstChild("Handle") then
					child.Handle.CFrame=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				end
			else
				if child.PrimaryPart~=nil then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=child.PrimaryPart.CFrame
				elseif child:FindFirstChild("Handle") then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=child.Handle.CFrame
				end
			end
		end)
		con2 = button.MouseButton2Click:Connect(function()
			local parent=button.Parent
			if skip~=true then
				table.remove(buttonCons,table.find(buttonCons,con))
				table.remove(buttonCons,table.find(buttonCons,con2))
			end
			button:Destroy()
			con:Disconnect() con=nil
			con2:Disconnect() con2=nil
			parent.CanvasSize=UDim2.new(0,0,0,parent.UIListLayout.AbsoluteContentSize.Y)
		end)
		if skip~=true then
			table.insert(buttonCons,con)
			table.insert(buttonCons,con2)
		end
	end)
end

local getFolder
getFolder = function(num,text)
	if num~=val then return end
	
	for i,v in pairs(buttonCons) do
		task.spawn(function()
			table.remove(buttonCons,v)
			v:Disconnect()
			v=nil
		end)
	end
	
	local found=false
	local oldFolder=folder
	if workspace:FindFirstChildOfClass("Tool") then folder=workspace found=true end
	if not found then
		for i,v in pairs(workspace:GetDescendants()) do
			if text==nil and v:IsA("Folder") and v:FindFirstChildOfClass("Tool") or text~=nil and v:IsA("Folder") and v.Name==text then
				folder=v break
			end
		end
	end
	if text~=nil and folder==oldFolder then return end
	if folder==oldFolder then wait(5) getFolder(num)
	else
		if addCon~=nil then addCon:Disconnect() addCon=nil end
		if rmCon~=nil then rmCon:Disconnect() rmCon=nil end

		for i,v in pairs(folder:GetChildren()) do
			if v:IsA("Tool") then
				local Return = true
				if (game.GameId==6099256669 or game.GameId==2217006564) then
					local num = 0
					for e,g in pairs(v:GetChildren()) do
						if g.ClassName=="Script" and g.Name=="Properties" then
							Return = false break
						elseif g.ClassName=="LocalScript" and g.Name=="Properties" then
							num+=1
						end
					end
					if num>=2 then Return=true end
				else
					Return=false
				end
				if Return then continue end
				local button=createItem(v,ScrollingFrame,true)
				buttonFunc(button,v)
				getNum(ScrollingFrame,folder)
			end
		end

		addCon=folder.ChildAdded:Connect(function(child)
			local Return = true
			if (game.GameId==6099256669 or game.GameId==2217006564) then
				for i,v in pairs(child:GetChildren()) do
					if v.ClassName=="Script" and v.Name=="Properties" then
						Return = false break
					elseif g.ClassName=="LocalScript" and g.Name=="Properties" then
						num+=1
					end
				end
				if num>=2 then Return=true end
			else
				Return=false
			end
			if Return or not child:IsA("Tool") then return end
			local button=createItem(child,ScrollingFrame,true)
			buttonFunc(button,child)
			getNum(ScrollingFrame,folder)
		end)
		rmCon=folder.ChildRemoved:Connect(function(child)
			getNum(ScrollingFrame,folder)
		end)
	end
end
getFolder(val)

TextBox.FocusLost:Connect(function(enter)
	if enter then
		val=val+1
		getFolder(val,TextBox.Text)
	end
end)

TextButton.MouseButton1Click:Connect(function()
	bring=not bring
	updateBring()
end)

game:GetService("UserInputService").InputBegan:Connect(function(key)
	if key.KeyCode.Name=="KeypadFive" then
		TPMenu.Enabled=not TPMenu.Enabled
	end
end)

if npcFolder~=nil then
	for i,v in pairs(npcFolder:GetChildren()) do
		if v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") then
			local button=createItem(v,ScrollingFrame_3,false)
			button.amt:Destroy()
			button.TextLabel.TextXAlignment = Enum.TextXAlignment.Center
			button.TextLabel.AnchorPoint = Vector2.new(.5,0)
			button.TextLabel.Position = UDim2.new(0.5,0,0,0)
			button.TextLabel.Size = UDim2.new(.95,0,1,0)
			buttonFunc(button,v,true)
		end
	end
end
